    "lint-all-libs": "npm run lint-angular-rating && lint-angular-console-logger",
    "test-angular-rating": "ng test angular-rating --browsers=ChromeHeadless",
    "test-angular-console-logger": "ng test angular-console-logger --browsers=ChromeHeadless",
    "pack-angular-logger": "cd dist/angular-console-logger && npm pack",
    "pack-angular-rating": "cd dist/angular-rating && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.4",
